
.passenger-list {
	color: #333;
	font-size: 28rpx;
	padding-bottom: 80rpx;
}
.passenger-item {
	display: flex;
	align-items: center;
	background: #fff;
	padding: 25rpx;
	margin-bottom: 8rpx;
}
.icon {
	width: 50rpx;
	height: 50rpx;
	margin-right: 25rpx;
}
.passenger-content {
	flex: 1;
	display: flex;
	align-items: center;
}
.content {
	flex: 1;
	line-height: 50rpx;
}
.arrow-icon {
	margin-right: 10rpx;
	width: 40rpx;
	height: 40rpx;
}


